{
	"name": "todo-client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prebuild": "yarn clean",
		"build": "webpack --config ./webpack.config.js --progress --color --display-error-details",
		"clean": "rimraf dist",
		"start": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
		"test": "jest"
	},
	"jest": {
		"moduleDirectories": [
			"./node_modules",
			"./src"
		],
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"transform": {
			"^.+\\.(ts)$": "ts-jest"
		},
		"testMatch": [
			"**/__tests__/**/*.ts",
			"**/?(*.)(spec|test).ts"
		]
	},
	"dependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.8",
		"@types/express-fileupload": "^1.1.4",
		"@types/jsonwebtoken": "^3.0.0",
		"@types/mongoose": "^5.7.36",
		"@types/multer": "^1.4.4",
		"bcrypt": "^5.0.0",
		"cloudinary": "^1.23.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.10.2",
		"multer": "^1.4.2",
		"prettier": "^2.1.1",
		"rxjs": "^6.1.0",
		"superagent": "^3.8.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.5",
		"@babel/node": "^7.10.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.11.5",
		"@types/jest": "^22.2.3",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/node": "^10.0.4",
		"@types/superagent": "^3.5.8",
		"babel-loader": "^8.0.0-beta.2",
		"jasmine-marbles": "^0.3.1",
		"jest": "^22.4.3",
		"rimraf": "^2.6.2",
		"ts-jest": "^22.4.5",
		"typescript": "^4.0.2",
		"webpack": "^4.8.0",
		"webpack-cli": "^2.1.3",
		"webpack-node-externals": "^1.7.2"
	}
}
